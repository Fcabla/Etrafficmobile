<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>TrafficIncidentsServlet</servlet-name>
		<display-name>TrafficIncidentsServlet</display-name>
		<description>Used to get the traffic incidents from the model</description>
		<servlet-class>webServlets.TrafficIncidentsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PushSubscriptionServlet</servlet-name>
		<display-name>PushSubscriptionServlet</display-name>
		<description></description>
		<servlet-class>webServlets.PushSubscriptionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PushNotificationServlet</servlet-name>
		<display-name>PushNotificationServlet</display-name>
		<description></description>
		<servlet-class>webServlets.PushNotificationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TrafficIncidentsServlet</servlet-name>
		<url-pattern>/TrafficIncidentsServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PushSubscriptionServlet</servlet-name>
		<url-pattern>/PushSubscriptionServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PushNotificationServlet</servlet-name>
		<url-pattern>/PushNotificationServlet</url-pattern>
	</servlet-mapping>
</web-app>